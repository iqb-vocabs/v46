	skos:notation "E 1";
	skos:broader n1:h4h;
	skos:prefLabel "beschreiben Phänomene und führen sie auf bekannte physikalische Zusammenhänge zurück"@de.
n1:d3f
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "E 2";
	skos:broader n1:h4h;
	skos:prefLabel "wählen Daten und Informationen aus verschiedenen Quellen zur Bearbeitung von Aufgaben und Problemen aus, prüfen sie auf Relevanz und ordnen sie,"@de.
n1:h3p
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "E 3";
	skos:broader n1:h4h;
	skos:prefLabel "verwenden Analogien und Modellvorstellungen zur Wissensgenerierung,"@de.
n1:p5d
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "E 4";
	skos:broader n1:h4h;
	skos:prefLabel "wenden einfache Formen der Mathematisierung an,"@de.
n1:z5c
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "E 5";
	skos:broader n1:h4h;
	skos:prefLabel "nehmen einfache Idealisierungen vor,"@de.
n1:y5e
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "E 6";
	skos:broader n1:h4h;
