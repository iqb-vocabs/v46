	skos:inScheme n0:;
	skos:notation "B 1";
	skos:broader n1:h4r;
	skos:prefLabel "zeigen an einfachen Beispielen die Chancen und Grenzen physikalischer Sichtweisen bei inner- und außerfachlichen Kontexten auf,"@de.
n1:r6s
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "B 2";
	skos:broader n1:h4r;
	skos:prefLabel "vergleichen und bewerten alternative technische Lösungen auch unter Berücksichtung physikalischer, ökonomischer, sozialer und ökologischer Aspekte,"@de.
n1:y4a
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "B 3";
	skos:broader n1:h4r;
	skos:prefLabel "nutzen physikalisches Wissen zum Bewerten von Risiken und Sicherheitsmaßnahmen bei Experimenten, im Alltag und bei modernen Technologien,"@de.
n1:c6c
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "B 4";
	skos:broader n1:h4r;
	skos:prefLabel "benennen Auswirkungen physikalischer Erkenntnisse in historischen und gesellschaftlichen Zusammenhängen."@de.
n1:h4r
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "B ";
